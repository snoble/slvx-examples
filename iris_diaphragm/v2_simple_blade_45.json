{
  "schema": "slvs-json/1",
  "units": "mm",
  "entities": [
    {
      "type": "point",
      "id": "origin",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "pivot",
      "at": [100, 0, 0]
    },
    {
      "type": "point",
      "id": "blade_tip",
      "at": [1, 1, 0]
    },
    {
      "type": "line",
      "id": "reference",
      "p1": "origin",
      "p2": "pivot"
    },
    {
      "type": "line",
      "id": "blade",
      "p1": "pivot",
      "p2": "blade_tip"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "origin"
    },
    {
      "type": "fixed",
      "entity": "pivot"
    },
    {
      "type": "distance",
      "between": ["pivot", "blade_tip"],
      "value": 80
    },
    {
      "type": "angle",
      "between": ["reference", "blade"],
      "value": 45
    }
  ]
}