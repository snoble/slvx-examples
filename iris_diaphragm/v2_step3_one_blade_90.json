{
  "schema": "slvs-json/1",
  "units": "mm",
  "parameters": {
    "blade_angle": 90
  },
  "entities": [
    {
      "type": "point",
      "id": "center",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "pivot1",
      "at": [100, 0, 0]
    },
    {
      "type": "point",
      "id": "pivot2",
      "at": [1, 1, 0]
    },
    {
      "type": "point",
      "id": "blade1_tip",
      "at": [1, 1, 0]
    },
    {
      "type": "line",
      "id": "spoke1",
      "p1": "center",
      "p2": "pivot1"
    },
    {
      "type": "line",
      "id": "spoke2",
      "p1": "center",
      "p2": "pivot2"
    },
    {
      "type": "line",
      "id": "blade1",
      "p1": "pivot1",
      "p2": "blade1_tip"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "center"
    },
    {
      "type": "fixed",
      "entity": "pivot1"
    },
    {
      "type": "distance",
      "between": ["center", "pivot2"],
      "value": 100
    },
    {
      "type": "angle",
      "between": ["spoke1", "spoke2"],
      "value": 60
    },
    {
      "type": "distance",
      "between": ["pivot1", "blade1_tip"],
      "value": 80
    },
    {
      "type": "angle",
      "between": ["spoke1", "blade1"],
      "value": "$blade_angle"
    }
  ]
}