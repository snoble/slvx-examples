{
  "schema": "slvs-json/1",
  "units": "mm",
  "parameters": {
    "pivot_radius": 100,
    "blade_length": 80,
    "blade_width": 25,
    "blade_angle": 75
  },
  "entities": [
    {
      "type": "point",
      "id": "center",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "pivot1",
      "at": [1, 1, 0]
    },
    {
      "type": "point",
      "id": "pivot2",
      "at": [1, 1, 0]
    },
    {
      "type": "point",
      "id": "blade1_tip",
      "at": [1, 1, 0]
    },
    {
      "type": "point",
      "id": "blade1_side",
      "at": [1, 1, 0]
    },
    {
      "type": "point",
      "id": "blade2_tip",
      "at": [1, 1, 0]
    },
    {
      "type": "point",
      "id": "blade2_side",
      "at": [1, 1, 0]
    },
    {
      "type": "line",
      "id": "spoke1",
      "p1": "center",
      "p2": "pivot1"
    },
    {
      "type": "line",
      "id": "spoke2",
      "p1": "center",
      "p2": "pivot2"
    },
    {
      "type": "line",
      "id": "blade1_leading",
      "p1": "pivot1",
      "p2": "blade1_tip"
    },
    {
      "type": "line",
      "id": "blade1_trailing",
      "p1": "pivot1",
      "p2": "blade1_side"
    },
    {
      "type": "line",
      "id": "blade1_inner",
      "p1": "blade1_tip",
      "p2": "blade1_side"
    },
    {
      "type": "line",
      "id": "blade2_leading",
      "p1": "pivot2",
      "p2": "blade2_tip"
    },
    {
      "type": "line",
      "id": "blade2_trailing",
      "p1": "pivot2",
      "p2": "blade2_side"
    },
    {
      "type": "line",
      "id": "blade2_inner",
      "p1": "blade2_tip",
      "p2": "blade2_side"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "center"
    },
    {
      "type": "horizontal",
      "a": "spoke1"
    },
    {
      "type": "distance",
      "between": ["center", "pivot1"],
      "value": "$pivot_radius"
    },
    {
      "type": "distance",
      "between": ["center", "pivot2"],
      "value": "$pivot_radius"
    },
    {
      "type": "angle",
      "between": ["spoke1", "spoke2"],
      "value": 60
    },
    {
      "type": "distance",
      "between": ["pivot1", "blade1_tip"],
      "value": "$blade_length"
    },
    {
      "type": "distance",
      "between": ["pivot2", "blade2_tip"],
      "value": "$blade_length"
    },
    {
      "type": "angle",
      "between": ["spoke1", "blade1_leading"],
      "value": "$blade_angle"
    },
    {
      "type": "angle",
      "between": ["spoke2", "blade2_leading"],
      "value": "$blade_angle"
    },
    {
      "type": "distance",
      "between": ["pivot1", "blade1_side"],
      "value": "$blade_width"
    },
    {
      "type": "distance",
      "between": ["pivot2", "blade2_side"],
      "value": "$blade_width"
    },
    {
      "type": "angle",
      "between": ["blade1_leading", "blade1_trailing"],
      "value": 20
    },
    {
      "type": "angle",
      "between": ["blade2_leading", "blade2_trailing"],
      "value": 20
    }
  ]
}