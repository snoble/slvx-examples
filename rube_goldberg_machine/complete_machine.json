{
  "schema": "slvs-json/1",
  "units": "mm",
  "parameters": {
    "marble_radius": 10,
    "ramp_angle": 30,
    "ramp_length": 300,
    "table_height": 1000,
    "ramp_start_height": 200,
    "drop_height": 150,
    "catcher_width": 60,
    "lever_length": 200,
    "lever_ratio": 3,
    "lever_height": 800,
    "domino_spacing": 30,
    "domino_count": 5,
    "domino_height": 40,
    "domino_width": 10,
    "pulley_height": 700,
    "weight_drop": 100,
    "bell_distance": 150
  },
  "entities": [
    {
      "type": "point",
      "id": "origin",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "table_top",
      "at": [0, "$table_height", 0]
    },
    {
      "type": "point",
      "id": "ramp_start",
      "at": [0, "$table_height + $ramp_start_height", 0]
    },
    {
      "type": "point",
      "id": "ramp_end",
      "at": [250, 1050, 0]
    },
    {
      "type": "line",
      "id": "ramp",
      "p1": "ramp_start",
      "p2": "ramp_end"
    },
    {
      "type": "point",
      "id": "horizontal_ref_end",
      "at": [100, "$table_height", 0]
    },
    {
      "type": "line",
      "id": "horizontal_ref",
      "p1": "table_top",
      "p2": "horizontal_ref_end"
    },
    {
      "type": "point",
      "id": "marble_exit",
      "at": [280, 1040, 0]
    },
    {
      "type": "point",
      "id": "catcher_center",
      "at": [350, 850, 0]
    },
    {
      "type": "point",
      "id": "catcher_left",
      "at": [320, 850, 0]
    },
    {
      "type": "point",
      "id": "catcher_right",
      "at": [380, 850, 0]
    },
    {
      "type": "line",
      "id": "catcher",
      "p1": "catcher_left",
      "p2": "catcher_right"
    },
    {
      "type": "point",
      "id": "lever_pivot",
      "at": [350, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "lever_left",
      "at": [300, 800, 0]
    },
    {
      "type": "point",
      "id": "lever_right",
      "at": [500, 800, 0]
    },
    {
      "type": "line",
      "id": "lever",
      "p1": "lever_left",
      "p2": "lever_right"
    },
    {
      "type": "point",
      "id": "lever_marble_target",
      "at": [320, 810, 0]
    },
    {
      "type": "point",
      "id": "next_trigger",
      "at": [480, 790, 0]
    },
    {
      "type": "point",
      "id": "domino1_base",
      "at": [520, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "domino1_top",
      "at": [520, 840, 0]
    },
    {
      "type": "line",
      "id": "domino1",
      "p1": "domino1_base",
      "p2": "domino1_top"
    },
    {
      "type": "point",
      "id": "domino2_base",
      "at": [550, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "domino2_top",
      "at": [550, 840, 0]
    },
    {
      "type": "line",
      "id": "domino2",
      "p1": "domino2_base",
      "p2": "domino2_top"
    },
    {
      "type": "point",
      "id": "domino3_base",
      "at": [580, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "domino3_top",
      "at": [580, 840, 0]
    },
    {
      "type": "line",
      "id": "domino3",
      "p1": "domino3_base",
      "p2": "domino3_top"
    },
    {
      "type": "point",
      "id": "domino4_base",
      "at": [610, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "domino4_top",
      "at": [610, 840, 0]
    },
    {
      "type": "line",
      "id": "domino4",
      "p1": "domino4_base",
      "p2": "domino4_top"
    },
    {
      "type": "point",
      "id": "domino5_base",
      "at": [640, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "domino5_top",
      "at": [640, 840, 0]
    },
    {
      "type": "line",
      "id": "domino5",
      "p1": "domino5_base",
      "p2": "domino5_top"
    },
    {
      "type": "point",
      "id": "pulley_center",
      "at": [700, "$pulley_height", 0]
    },
    {
      "type": "point",
      "id": "weight_top",
      "at": [700, "$pulley_height - 50", 0]
    },
    {
      "type": "point",
      "id": "weight_bottom",
      "at": [700, 550, 0]
    },
    {
      "type": "line",
      "id": "weight_string",
      "p1": "weight_top",
      "p2": "weight_bottom"
    },
    {
      "type": "point",
      "id": "bell_position",
      "at": [850, "$lever_height", 0]
    },
    {
      "type": "point",
      "id": "bell_striker",
      "at": [820, "$lever_height", 0]
    },
    {
      "type": "line",
      "id": "bell_arm",
      "p1": "pulley_center",
      "p2": "bell_striker"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "origin"
    },
    {
      "type": "fixed",
      "entity": "table_top"
    },
    {
      "type": "fixed",
      "entity": "ramp_start"
    },
    {
      "type": "distance",
      "between": ["ramp_start", "ramp_end"],
      "value": "$ramp_length"
    },
    {
      "type": "angle",
      "between": ["horizontal_ref", "ramp"],
      "value": "-$ramp_angle"
    },
    {
      "type": "distance",
      "between": ["ramp_end", "marble_exit"],
      "value": "40"
    },
    {
      "type": "horizontal",
      "entity": "catcher"
    },
    {
      "type": "distance",
      "between": ["catcher_left", "catcher_right"],
      "value": "$catcher_width"
    },
    {
      "type": "distance",
      "between": ["marble_exit", "catcher_center"],
      "value": "$drop_height"
    },
    {
      "type": "coincident",
      "at": "catcher_center",
      "of": ["catcher"]
    },
    {
      "type": "fixed",
      "entity": "lever_pivot"
    },
    {
      "type": "coincident",
      "at": "lever_pivot",
      "of": ["lever"]
    },
    {
      "type": "distance",
      "between": ["lever_left", "lever_right"],
      "value": "$lever_length"
    },
    {
      "type": "coincident",
      "at": "lever_marble_target",
      "of": ["lever"]
    },
    {
      "type": "coincident",
      "at": "next_trigger",
      "of": ["lever"]
    },
    {
      "type": "distance",
      "between": ["catcher_center", "lever_marble_target"],
      "value": "50"
    },
    {
      "type": "vertical",
      "entity": "domino1"
    },
    {
      "type": "vertical",
      "entity": "domino2"
    },
    {
      "type": "vertical",
      "entity": "domino3"
    },
    {
      "type": "vertical",
      "entity": "domino4"
    },
    {
      "type": "vertical",
      "entity": "domino5"
    },
    {
      "type": "distance",
      "between": ["domino1_base", "domino1_top"],
      "value": "$domino_height"
    },
    {
      "type": "distance",
      "between": ["domino2_base", "domino2_top"],
      "value": "$domino_height"
    },
    {
      "type": "distance",
      "between": ["domino3_base", "domino3_top"],
      "value": "$domino_height"
    },
    {
      "type": "distance",
      "between": ["domino4_base", "domino4_top"],
      "value": "$domino_height"
    },
    {
      "type": "distance",
      "between": ["domino5_base", "domino5_top"],
      "value": "$domino_height"
    },
    {
      "type": "distance",
      "between": ["next_trigger", "domino1_base"],
      "value": "45"
    },
    {
      "type": "distance",
      "between": ["domino1_base", "domino2_base"],
      "value": "$domino_spacing"
    },
    {
      "type": "distance",
      "between": ["domino2_base", "domino3_base"],
      "value": "$domino_spacing"
    },
    {
      "type": "distance",
      "between": ["domino3_base", "domino4_base"],
      "value": "$domino_spacing"
    },
    {
      "type": "distance",
      "between": ["domino4_base", "domino5_base"],
      "value": "$domino_spacing"
    },
    {
      "type": "horizontal",
      "entity": "bell_arm"
    },
    {
      "type": "fixed",
      "entity": "pulley_center"
    },
    {
      "type": "vertical",
      "entity": "weight_string"
    },
    {
      "type": "distance",
      "between": ["weight_top", "weight_bottom"],
      "value": "$weight_drop"
    },
    {
      "type": "distance",
      "between": ["domino5_base", "weight_top"],
      "value": "80"
    },
    {
      "type": "fixed",
      "entity": "bell_position"
    },
    {
      "type": "distance",
      "between": ["pulley_center", "bell_striker"],
      "value": "$bell_distance"
    }
  ]
}