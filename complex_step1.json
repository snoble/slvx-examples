{
  "schema": "slvs-json/1",
  "units": "mm",
  "entities": [
    {
      "type": "point",
      "id": "A",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "B",
      "at": [1, 0, 0]
    },
    {
      "type": "point",
      "id": "C",
      "at": [0, 1, 0]
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "A"
    },
    {
      "type": "distance",
      "between": ["A", "B"],
      "value": 100
    },
    {
      "type": "distance",
      "between": ["B", "C"],
      "value": 80
    },
    {
      "type": "distance",
      "between": ["C", "A"],
      "value": 60
    }
  ]
}